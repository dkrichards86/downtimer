{
  "name": "downtimer",
  "version": "0.1.0",
  "private": true,
  "description": "DownTimer.io, a browser based productivity timer.",
  "author": "Keith Richards <dkrichards86@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "^2.7.3",
    "http-server": "^0.11.1",
    "lodash": "^4.17.11",
    "moment": "2.21.0",
    "push.js": "^1.0.9",
    "register-service-worker": "^1.6.1",
    "shortid": "^2.2.14",
    "vue": "^2.6.6",
    "vue-chartjs": "^3.4.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.1",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.6",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ajv": "^6.9.1",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.14.0",
    "eslint-plugin-vue": "^5.2.1",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.6",
    "vuetify-loader": "^1.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-plusplus": 0,
      "comma-dangle": 0,
      "arrow-body-style": 0,
      "arrow-parens": 0,
      "object-shorthand": 0,
      "func-names": 0,
      "import/first": 0,
      "import/named": 2,
      "import/namespace": 2,
      "import/default": 2,
      "import/export": 2,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
